{% extends "base.html.twig"  %}
{% block body %}

<table class="table">
<thead>
<td scope="col">ID</td>
<td scope="col">Name</td>
<td scope="col">Description</td>
<td scope="col">Category</td>
<td scope="col">Sous Category</td>
<td scope="col">Image</td>
<th scope="col">Edit</th>
<th scope="col">Supprimer</th>
</thead>
<tbody >
{% for j in automobiles %}
<tr>
<th scope="row">{{j.Id}}</th>
<td>{{j.Nimmatriculation}}</td>
<td>{{j.marque.getNomMarque()}}</td>
<td>{{j.modele.getNomModele()}}</td>
<td>{{j.categorie.getNomCategorie}}</td>
<td><img src="{{asset('uploads/')}}{{j.Image}}" style="width:300px;height:300px" alt="" > </td>
<td class="green"><a class="btn btn-info" href="{{path('automobile_edit', {'id': j.id})}}">Modifier</a></td>
<td class="green"><a class = "btn btn-danger" href="{{path('automobile_delete',{'id':j.id})}}">Supprimer</a></td>
</tr>
{% endfor %}
</tbody>
</table>

{% endblock %}